@namespace BlastUI.Components
@inherits ComponentBase
<fast-anchor appearance="@Utils.GetEnumName(Appearance)" href="#" autofocus="@Autofocus" disabled="@Disabled">
    @if (Start != null)
    {
        <slot name="start">@Start</slot>
    }
    @ChildContent
    @if (End != null)
    {
        <slot name="end">@End</slot>
    }
</fast-anchor>
