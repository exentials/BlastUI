@namespace BlastUI.Components
@inherits ComponentBase

<fast-button appearance="@Appearance" autofocus="@Autofocus" disabled="@Disabled" @onclick="Click">
    @if (Start != null)
    {
        <slot name="start">@Start</slot>
    }
    @ChildContent
    @if (End != null)
    {
        <slot name="end">@End</slot>
    }
</fast-button>
